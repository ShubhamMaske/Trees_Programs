class Solution{
    public:
    //Function to find the height of a binary tree.
    int height(struct Node* node){ 
       if(node==NULL)
        return 0;
     
      int l=height(node->left);  //Recursion for left side height
      int r=height(node->right); //Recursion for right side height
      int ans=max(l,r)+1;
      return ans;
    }
};
